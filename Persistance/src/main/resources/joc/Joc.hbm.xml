<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name = "joc.Joc"
           table="Joc">
        <id name = "id" column = "id">
            <generator class="increment"/>
        </id>
        <property name="idJoc" column = "idJoc"/>
        <property name="username" column="username"/>
        <property name="poz1" column="poz1"/>
        <property name="poz2" column="poz2"/>
        <property name="poz3" column="poz3"/>

    </class>

</hibernate-mapping>